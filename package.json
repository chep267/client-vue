        "autoprefixer": "^10.4.20",
        "eslint": "^9.19.0",
        "eslint-plugin-vue": "^9.32.0",
        "prettier": "^3.4.2",
        "sass-embedded": "^1.83.4",
        "typescript": "5.7.3",
        "vite": "^6.0.11",
        "vite-plugin-vue-devtools": "^7.7.1",
        "vite-plugin-vuetify": "^2.0.4",
        "vue-tsc": "^2.2.0"
