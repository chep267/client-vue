        "autoprefixer": "^10.4.20",
        "eslint": "^9.16.0",
        "eslint-plugin-vue": "^9.32.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "sass-embedded": "^1.82.0",
        "typescript": "5.6.3",
        "vite": "^6.0.2",
        "vite-plugin-vue-devtools": "^7.6.7",
        "vite-plugin-vuetify": "^2.0.4",
        "vue-tsc": "^2.1.10"
