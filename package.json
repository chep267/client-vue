        "@vueuse/core": "^12.0.0",
        "axios": "^1.7.9",
        "dayjs": "^1.11.13",
        "firebase": "^11.0.2",
        "js-cookie": "^3.0.5",
        "pinia": "^2.3.0",
        "tailwindcss": "^3.4.16",
        "vee-validate": "^4.14.7",
        "vue": "^3.5.13",
        "vue-i18n": "10.0.5",
        "vue-router": "^4.5.0",
        "vuetify": "^3.7.5"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.4",
        "@types/js-cookie": "^3.0.6",
