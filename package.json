        "@mdi/js": "^7.4.47",
        "@tailwindcss/vite": "^4.0.3",
        "@tanstack/vue-query": "^5.66.0",
        "@tsparticles/slim": "^3.8.1",
        "@tsparticles/vue3": "^3.0.1",
        "@vueuse/core": "^12.5.0",
        "axios": "^1.7.9",
        "dayjs": "^1.11.13",
        "firebase": "^11.2.0",
        "js-cookie": "^3.0.5",
        "pinia": "^2.3.1",
        "tailwindcss": "^4.0.3",
        "vee-validate": "^4.15.0",
        "vue": "^3.5.13",
        "vue-i18n": "11.1.0",
        "vue-router": "^4.5.0",
        "vuetify": "^3.7.9"
