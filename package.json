{
    "name": "chep-client",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev-server": "cd ../express-server && pnpm dev",
        "dev:debug": "DEBUG=\"vite:transform\" vite",
        "build": "rm -rf dist && vue-tsc && vite build  && cp dist/index.html dist/404.html",
        "build:staging": "rm -rf dist && vue-tsc && vite build --mode staging",
        "build:dev": "rm -rf dist && vue-tsc && vite build --mode development",
        "preview": "vite preview",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss|*.vue)\"",
        "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss|*.vue)\"",
        "predeploy": "pnpm run build",
        "deploy": "gh-pages -d dist",
        "update": "ncu -u && pnpm i",
        "analyze": "source-map-explorer 'dist/assets/*.js'"
    },
    "dependencies": {
        "@eslint/js": "^9.11.1",
        "@mdi/js": "^7.4.47",
        "@tanstack/vue-query": "^5.56.2",
        "@tsparticles/slim": "^3.5.0",
        "@tsparticles/vue3": "^3.0.1",
        "@types/js-cookie": "^3.0.6",
        "@vueuse/core": "^11.1.0",
        "@vueuse/integrations": "^11.1.0",
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "firebase": "^10.14.0",
        "js-cookie": "^3.0.5",
        "material-design-icons-iconfont": "^6.7.0",
        "pinia": "^2.2.4",
        "tailwindcss": "^3.4.13",
        "universal-cookie": "^7.2.0",
        "vee-validate": "^4.13.2",
        "vue": "^3.5.10",
        "vue-i18n": "10.0.0-beta.1",
        "vue-router": "^4.4.5",
        "vuetify": "^3.7.2"
    },
    "devDependencies": {
        "@types/node": "^22.7.4",
        "@typescript-eslint/eslint-plugin": "^8.8.0",
        "@typescript-eslint/parser": "^8.8.0",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.28.0",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "rollup-plugin-visualizer": "^5.12.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "5.6.2",
        "typescript-eslint": "^8.8.0",
        "vue-tsc": "^2.1.6",
        "vite": "^5.4.8",
        "vite-plugin-vuetify": "^2.0.4",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^5.1.4"
    }
}
