        "update-all": "pnpm up --latest",
        "deploy": "pnpm build && gh-pages -d dist",
        "analyze": "open dist/stats.html",
        "check-code": "node script/check-coding-rules.ts"
    },
    "dependencies": {
        "@angelblanco/v-calendar": "^3.1.3",
        "@mdi/js": "^7.4.47",
        "@tanstack/vue-query": "^5.83.1",
        "@tsparticles/slim": "^3.9.1",
        "@tsparticles/vue3": "^3.0.1",
        "@vueuse/core": "^13.6.0",
        "axios": "^1.11.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "js-cookie": "^3.0.5",
        "pinia": "^3.0.3",
        "tailwindcss": "^4.1.11",
        "vee-validate": "^4.15.1",
        "vue": "^3.5.18",
        "vue-i18n": "11.1.11",
        "vue-router": "^4.5.1",
        "vuetify": "^3.9.4"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "@tsconfig/node24": "^24.0.1",
