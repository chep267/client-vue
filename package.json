{
    "name": "chep-client",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:debug": "DEBUG=\"vite:transform\" vite",
        "build": "rm -rf dist && vue-tsc && vite build  && cp dist/index.html dist/404.html",
        "build:staging": "rm -rf dist && vue-tsc && vite build --mode staging",
        "build:dev": "rm -rf dist && vue-tsc && vite build --mode development",
        "preview": "vite preview",
        "lint": "eslint . --ext js,jsx,ts,tsx,vue --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx,vue src/",
        "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss|*.vue)\"",
        "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss|*.vue)\"",
        "predeploy": "pnpm run build",
        "deploy": "gh-pages -d dist",
        "update": "ncu -u && npm i --legacy-peer-deps",
        "analyze": "source-map-explorer 'dist/assets/*.js'"
    },
    "dependencies": {
        "@mdi/js": "^7.4.47",
        "@tanstack/vue-query": "^5.26.3",
        "@tsparticles/slim": "^3.3.0",
        "@tsparticles/vue3": "^3.0.1",
        "@types/js-cookie": "^3.0.6",
        "@vueuse/core": "^10.9.0",
        "@vueuse/integrations": "^10.9.0",
        "axios": "^1.6.7",
        "firebase": "^10.8.1",
        "js-cookie": "^3.0.5",
        "material-design-icons-iconfont": "^6.7.0",
        "pinia": "^2.1.7",
        "tailwindcss": "^3.4.1",
        "universal-cookie": "^6.1.3",
        "vee-validate": "^4.12.6",
        "vue": "^3.4.21",
        "vue-i18n": "^9.10.1",
        "vue-router": "^4.3.0",
        "vuetify": "^3.5.8"
    },
    "devDependencies": {
        "@types/node": "^20.11.26",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "autoprefixer": "^10.4.18",
        "babel-plugin-direct-import": "^1.0.0",
        "babel-plugin-transform-imports": "^2.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.23.0",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "^5.4.2",
        "vite": "^5.1.6",
        "vite-plugin-vuetify": "^2.0.3",
        "vue-tsc": "^1.8.27"
    }
}
