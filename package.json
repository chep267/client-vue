{
    "name": "chep-client-vue",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev-server": "cd ../express-server && pnpm dev",
        "type-check": "vue-tsc -b",
        "build": "rimraf dist && vue-tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "update-all": "pnpm up --latest",
        "deploy": "pnpm build && gh-pages -d dist",
        "analyze": "open dist/stats.html",
        "check-code": "node script/check-coding-rules.ts"
    },
    "dependencies": {
        "@angelblanco/v-calendar": "^3.1.3",
        "@mdi/js": "^7.4.47",
        "@tailwindcss/vite": "^4.1.8",
        "@tanstack/vue-query": "^5.80.6",
        "@tsparticles/slim": "^3.8.1",
        "@tsparticles/vue3": "^3.0.1",
        "@vueuse/core": "^13.3.0",
        "axios": "^1.9.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "js-cookie": "^3.0.5",
        "pinia": "^3.0.3",
        "tailwindcss": "^4.1.8",
        "vee-validate": "^4.15.1",
        "vue": "^3.5.16",
        "vue-i18n": "11.1.5",
        "vue-router": "^4.5.1",
        "vuetify": "^3.8.8"
    },
    "devDependencies": {
        "@tsconfig/node24": "^24.0.1",
