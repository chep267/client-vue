{
    "name": "chep-client-vue",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev-server": "cd ../express-server && pnpm dev",
        "type-check": "vue-tsc -b",
        "build": "rimraf dist && vue-tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "update-all": "pnpm up --latest",
        "deploy": "pnpm build && gh-pages -d dist",
        "analyze": "open dist/stats.html",
        "check-code": "node script/check-coding-rules.ts"
    },
    "dependencies": {
        "@mdi/js": "^7.4.47",
        "@popperjs/core": "^2.11.8",
        "@tanstack/vue-query": "^5.91.2",
        "@tsparticles/slim": "^3.9.1",
        "@tsparticles/vue3": "^3.0.1",
        "@vueuse/core": "^14.0.0",
        "axios": "^1.13.2",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.19",
        "js-cookie": "^3.0.5",
        "pinia": "^3.0.4",
        "tailwindcss": "^4.1.17",
        "v-calendar": "^3.1.2",
        "vee-validate": "^4.15.1",
        "vue": "^3.5.24",
        "vue-i18n": "11.1.12",
        "vue-router": "^4.6.3",
        "vuetify": "^3.10.11"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.1.17",
        "@tsconfig/node24": "^24.0.2",
